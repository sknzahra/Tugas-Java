public class LatihanJava {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        int number = 39;
        
        if(number < 10){
            System.out.println("I Love Programming");
        }
        else if (number < 30){
            System.out.println("I Love Programming 2nd");
        }
        //kalo ga masuk ke 2 kondisi diatas, masuk ke kondisinya else
        else{
            System.out.println("I Still Love Programming");
        }
        
        //cheatsheet = simplify = one line condition
        int number1 = 99;
        
        String hello = (number < 10) ?
                "Hello" : "Not Hello";
        System.out.println(hello);
    }
}
